{
  "name": "morgan-mongo-demo",
  "version": "1.0.0",
  "description": "Demo server for morgan-mongo middleware with geo-ip functionality",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "build": "npm run lint && tsc",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "heroku-postbuild": "npm run build",
    "start": "node dist/server.js",
    "watch": "npm run build && tsc -w",
    "dev": "ts-node-dev src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmstss/morgan-mongo-demo.git"
  },
  "keywords": [
    "morgan-mongo",
    "mongoose",
    "mongo",
    "middleware",
    "express",
    "typescript"
  ],
  "author": "Viachaslau Tyshkavets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmstss/morgan-mongo-demo/issues"
  },
  "homepage": "https://github.com/pmstss/morgan-mongo-demo#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.3.1",
    "@types/morgan": "^1.7.35",
    "@types/request": "^2.48.1",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.31",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "express": "^4.16.4",
    "is-local-ip": "^1.1.0",
    "mongoose": "^5.3.14",
    "morgan": "^1.9.1",
    "morgan-mongo": "^1.1.0",
    "request": "^2.88.0"
  },
  "engines": {
    "node": "10.x"
  }
